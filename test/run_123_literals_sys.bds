#!/usr/bin/env bds

hi := "Hello"
# These lines depend on the shell being used
# Using the default '/bin/sh' I see a 'tab' charater, whereas 
# using /bin/bash I see a '\t' (escaped tab)
sys  echo "sys                |\t|"
sys  echo "sys                |\t|    variable:$hi"
sys  echo "sys                |\\t|   variable:$hi"

