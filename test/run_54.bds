#!/usr/bin/env bds

string{} hash
hash{"hi"} = "bye" 
hash{"hola"} = "chau" 
print("Hash: $hash\n")

# Test iteration on a hash (iterates on values)
# Add all values from hash
string[] vals
for( string k : hash.keys() ) {
	print("Key : $k\n")
	vals.add(k)
}

vals = vals.sort()
print("List: $vals\n")

