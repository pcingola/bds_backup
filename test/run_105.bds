
prefix := "test/run_105"

# Prepare data ('par' should never run)
in  := "$prefix\.in"
out := "$prefix\.out"

sys date > $in
sleep(1)
sys date > $out

# Parallel
parId := par(false, out <- in) {
	for(int i : range(1,9)) {
		print("par: $i\n")
		sleep(0.1)
	}

	sys cat $in > $out
}

isRun := !parId.isEmpty()
print("parId : '$parId'\tisRun : $isRun\n")

# Wait for process to finish
wait parId
print("Finished: '$parId'\n")

