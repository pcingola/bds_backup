#!/usr/bin/env bds

string zzzCheck(int num) {
	num += 1
	checkpoint("test/checkpoint_27.chp")
	return "z" + num
}

int zzz(string in) {
	out := 1

	switch( in ) {
		case 'a':
			out *= 3
			break

		case zzzCheck(41):
			out *= 5

		case 'b':
			out *= 7
			break

		default:
			out *= 100
	}
	return out
}

# Main
out := zzz('z42')
