#!/usr/bin/env bds


int zzzCheck(int num) {
	num += 1
	checkpoint("test/checkpoint_26.chp")
	return num + 2
}

int zzz(string in) {
	out := 1

	switch( in ) {
		case 'a':
			out *= 3
			out = zzzCheck(out)
			break

		case 'z'+'x':
			out *= 5
			out = zzzCheck(out)

		case 'b':
			out *= 7
			break

		default:
			out *= 100
			out = zzzCheck(out)
	}
	return out
}

# Main
out := zzz('zx')
